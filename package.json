{
  "name": "chalkdust",
  "version": "2.1.20",
  "type": "module",
  "scripts": {
    "start": "npm run dev",
    "dev": "vite",
    "dev:function": "nodemon --inspect=0.0.0.0 --watch function/index.js node_modules/@google-cloud/functions-framework --target=sendgrid",
    "build": "vite build",
    "format": "prettier --write src --ignore-path ./.gitignore",
    "lint": "NODE_ENV=development eslint --ext .jsx src",
    "serve": "vite preview"
  },
  "dependencies": {
    "@arcgis/core": "^4.32.10",
    "@sendgrid/client": "^8.1.5",
    "@tailwindcss/vite": "^4.1.6",
    "@ugrc/eslint-config": "^1.2.2",
    "firebase": "^11.7.1",
    "proj4": "^2.16.2",
    "query-string": "^9.1.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^6.0.0",
    "vite-plugin-package-version": "^1.1.0",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.27.1",
    "@google-cloud/functions-framework": "^4.0.0",
    "@vitejs/plugin-react": "^4.4.1",
    "dotenv": "^16.5.0",
    "eslint": "^9.26.0",
    "firebase-tools": "^14.3.1",
    "http-server": "^14.1.1",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-packagejson": "^2.5.12",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.1.6",
    "vite": "^6.3.5"
  }
